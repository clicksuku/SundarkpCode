{   
   "name": "Rediff Viewer",   
   "description": "Opens Rediff pages in Print Mode",   	
   "action"  : 
   {
      "default_icon": "icon.png",
	  "default_title": "Rediff Viewer"	  
   },   
   "manifest_version"	: 3,   
	"version"			: "1.4",
	"icons"				: {				
		"128": "icon.png" 
	},   
   "content_scripts" : 
   [
		{
		  "matches": ["*://*.rediff.com/*"],
		  "js": ["jquery.min.js","rediff.js"]
		}
	]	
}
