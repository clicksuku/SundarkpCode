{   
	"background": {
		"page"	:	"background.html"
	},
	"browser_action"  : 
	{
      "default_icon": "open.png",
	  "default_title": "Lock Tabs",
	  "default_popup": "popup.html"	  
	},	   	
	"description": "Lock Tabs for switching. Press Ctrl+Z to switch.",   	      
	"icons"	: {				
		"128": "open.png" 
	},
	"manifest_version"	: 2, 
	"name": "LockTabs2Switch",   		
	"permissions": [ "tabs", "notifications" ],
	"update_url": "http://clients2.google.com/service/update2/crx",
	"version": "1.0.0",
	"commands": {
		"switch": {
		  "suggested_key": { "default": "Ctrl+Z" },
		  "description": "Switch only selected tabs"
		}
	}
}
